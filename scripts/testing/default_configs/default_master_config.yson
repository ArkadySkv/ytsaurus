{
  "meta_state" = {
    "cell" = {
      "addresses" = [
        "meta01-001g:9091";
        "meta01-002g:9091";
        "meta01-003g:9091";
      ];
    };
    "changelogs" = {
        "path" = "/yt/disk1/data/control/server/master/0/logs";
    };
    "snapshots" = {
        "path" = "/yt/disk1/data/control/server/master/0/snapshots";
    };
    "max_changes_between_snapshots" = 1000000;
  };
  transactions = {
    default_transaction_timeout = 300000;
  };
    logging = {
        rules = [
            {
                min_level = Info;
                writers = [ file ];
                categories  = [ "*" ];
            };
            {
                min_level = Debug;
                writers = [ raw ];
                categories  = [ "*" ];
            };
        ];
        writers = {
            stderr = {
                type = StdErr;
                pattern = "$(datetime) $(level) $(category) $(message)";
            };
            file = {
                type = File;
                file_name = "master-0.log";
                pattern = "$(datetime) $(level) $(category) $(message)";
            };
            raw = {
                type = raw;
                file_name = "master-0.debug.log";
            };
        }
  };
}
