{
  "meta_state" = {
    "cell" = {
      "addresses" = [
        "meta01-001g:9091";
        "meta01-002g:9091";
        "meta01-003g:9091";
      ];
    };
    "log_path" = "/yt/disk1/data/control/server/master/0/logs";
    "snapshot_path" = "/yt/disk1/data/control/server/master/0/snapshots";
    "max_changes_between_snapshots" = 1000000;
  };
  transactions = {
    default_transaction_timeout = 300000;
  };
  "logging" = {
    "rules" = [
      {
        "min_level" = "info";
        "writers" = [
          "file";
        ];
        "categories" = [
          "*";
        ];
      };
    ];
    "writers" = {
      "file" = {
        "file_name" = "master-0.log";
        "type" = "file";
        "pattern" = "$(datetime) $(level) $(category) $(message)";
      };
    };
  };
}