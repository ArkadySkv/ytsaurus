{
  "chunk_holder" = {
    "cache_remote_reader" = {
      "publish_peer" = "true";
    };
    "response_throttling_size" = 524288000;
    "max_cached_blocks_size" = 10737418240;
    "store_locations" = [
      {
        "path" = "/yt/disk1/data/chunk_store";
        "quota" = 1825361100800;
      };
      {
        "path" = "/yt/disk2/data/chunk_store";
        "quota" = 1825361100800;
      };
      {
        "path" = "/yt/disk3/data/chunk_store";
        "quota" = 1825361100800;
      };
      {
        "path" = "/yt/disk4/data/chunk_store";
        "quota" = 1825361100800;
      };
    ];
    "cache_location" = {
      "path" = "/yt/disk1/data/chunk_cache";
      "quota" = 1073741824;
    };
    "full_heartbeat_timeout" = 180000;
    "max_cached_readers" = 256;
  };
  "masters" = {
    "addresses" = [
      "meta01-001g:9091";
      "meta01-002g:9091";
      "meta01-003g:9091";
    ];
  };
  "logging" = {
    "rules" = [
      {
        "min_level" = "info";
        "writers" = [
          "file";
        ];
        "categories" = [
          "*";
        ];
      };
    ];
    "writers" = {
      "file" = {
        "file_name" = "holder-0.log";
        "type" = "file";
        "pattern" = "$(datetime) $(level) $(category) $(message)";
      };
    };
  };
}