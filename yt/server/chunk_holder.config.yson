{
    masters = {
        addresses = ["localhost:8001", "localhost:8002", "localhost:8003" ]
    };

    chunk_holder = {
        store_locations = [ {path = "node1"} ];
        cache_locations = {path = "cache1"};
    }

    exec_agent = {
    };

    "logging"= {
        "rules"= [
            {
                "writers"= ["file"];
                "categories"= ["*"];
                "min_level"= "Debug"
            };
            {
                "writers"= ["stderr"];
                "categories"= ["*"];
                "min_level"= "Info"
            }
        ];
        "writers"= {
            "file" = {
                "pattern"= "$(datetime) $(level) $(category) $(message)";
                "type"= "File";
                "file_name"= "chunk_holder.log"
            };

            "stderr" = {
                "pattern"= "$(datetime) $(level) $(category) $(message)";
                "type"= "StdErr";
            }
        }
    }
}
