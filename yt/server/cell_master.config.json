{
    "MetaState": {
        "Cell": {
            "Addresses": [
                "localhost:8000",
                "localhost:8001",
                "localhost:8002"
            ]
        },
        "SnapshotLocation": ".",
        "MaxChangesBetweenSnapshots": 1000000,
        "LogLocation": "."
    },
    "Logging": {
        "Rules": [
            {
                "Writers": [
                    "File",
                    "StdErr"
                ],
                "Categories": [
                    "*"
                ],
                "MinLevel": "Debug"
            }
        ],
        "Writers": [
            {
                "Pattern": "$(datetime) $(level) $(category) $(message)",
                "Type": "File",
                "Name": "File",
                "FileName": "master.log"
            },
            {
                Name : "StdErr",
                Type : "StdErr",
                Pattern : "$(datetime) $(level) $(category) $(message)"
            }
        ]
    }
}
